@model DomainModels.Models.User
@{
    ViewBag.Title = "About_User";
}

<h2>About_User</h2>

<div class="row">



    <div class="col-md-4">
        <p>@Model.FIO</p>
        <p>@Model.Login</p>
        <p>@Model.Password</p>
        <p>@Model.Id</p>
    </div>


    @foreach (var or in Model.OperationResults)
    {
        <li>@or.Result</li>
    }

</div>